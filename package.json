{
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6"
  },
  "scripts": {
    "publish": "postcss ./resources/tailwind.main.css -o ./public/styles/app.css",
    "build": "tailwind build -i ./resources/tailwind.main.css -o ./public/styles/app.css --minify",
    "watch": "tailwind -i ./resources/tailwind.main.css -o ./public/styles/app.css -w"
  },
  "dependencies": {
    "cssnano": "^5.1.12",
    "postcss-cli": "^10.0.0"
  }
}
